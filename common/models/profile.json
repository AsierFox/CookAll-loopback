{
  "name": "Profile",
  "plural": "profiles",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "length": 50
    },
    "surnames": {
      "type": "string",
      "required": true,
      "length": 100
    },
    "gender": {
      "type": "string",
      "enum": [
        "m",
        "f"
      ],
      "length": 1
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "dataType": "datetime",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "recipes": {
      "type": "hasMany",
      "model": "Recipe",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
