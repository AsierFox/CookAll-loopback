{
  "name": "Chat",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true,
      "index": true,
      "length": 100
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "dataType": "datetime",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "messages": {
      "type": "hasMany",
      "model": "ChatMessage",
      "foreignKey": "messageId"
    }
  },
  "acls": [],
  "methods": {}
}
