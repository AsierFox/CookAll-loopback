{
  "name": "Feedback",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "subject": {
      "type": "string",
      "required": true,
      "index": true,
      "length": 150
    },
    "description": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "dataType": "datetime",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "profile": {
      "type": "belongsTo",
      "model": "Profile",
      "foreignKey": "profileId"
    }
  },
  "acls": [],
  "methods": {}
}
